FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform -30000;

boundaryField
{
    solution_inlet
    {
        type            fixedValue;
	value 		uniform 0.0;
    }
    outlet
    {
        type            zeroGradient;
    }

    walls
    {
        type            zeroGradient;
    }

    water_inlet
    {
        type            fixedValue;
	value		uniform -50196.0;
    }
    symmetry_plane
    {
      type		symmetry;
    }
}

