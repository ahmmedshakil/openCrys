FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      P1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.5;

boundaryField
{
    sup_side_inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    inf_side_inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    outlet
    {
        type            zeroGradient;
    }
    symmetry_plane
    {
        type            symmetry;
    }
    walls
    {
        type            zeroGradient;
    }
    solution_inlet
    {
        type            fixedValue;
        value           uniform 1.0;
    }
}


